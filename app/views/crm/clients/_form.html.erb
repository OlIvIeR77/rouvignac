
<ul class="form-group">
    <%= f.label 'last_name' %>
  <li class="form-control">
    <%= f.text_field :last_name, class: 'twb_form_control'  %>
  </li>
  <li class="form-control">
    <%= f.label 'first_name' %>
    <%= f.text_field :first_name, class: 'twb_form_control' %>
  </li>
  <li class="form-control">
    <%= f.label 'email' %>
    <%= f.email_field :email, class: 'twb_form_control'  %>
  </li>
  <li class="form-control">
    <%= f.label 'phone_number' %>
    <%= f.text_field :phone_number, class: 'twb_form_control'  %>
  </li>
  <li class="form-control">
    <%= f.label 'mobile_phone_number' %>
    <%= f.text_field :mobile_phone_number, class: 'twb_form_control'  %>
  </li>
  <li class="form-control">
    <%= f.label 'street_number' %>
    <%= f.number_field :street_number, class: 'twb_number_field' %>
  </li>
  <li class="form-control">
    <%= f.label 'street_name' %>
    <%= f.text_field :street_name, class: 'twb_form_control'  %>
  </li>
  <li class="form-control">
    <%= f.label 'postal_code' %>
    <%= f.number_field :postal_code, class: 'twb_number_field' %>
  </li>
  <li class="form-control">
    <%= f.label 'city' %>
    <%= f.text_field :city, class: 'twb_form_control'  %>
  </li>
  <li class="form-control">
    <%= f.label 'number of Persons' %>
    <%= f.number_field :number_of_person, class: 'twb_number_field' %>
  </li>
  <li>
    <%= f.submit 'submit', class: 'btn btn-success submit_button' %>
  </li>
</ul>
